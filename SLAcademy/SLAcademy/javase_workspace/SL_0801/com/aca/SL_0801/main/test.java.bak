package com.aca.SL_0801.main;
public class Arrow 
{
	int y;
	String str;
	int velX;
	int x;
	public Arrow(int y, String str, int velX) {
			this.y = y;
			this.str = str;
			this.velX = velX;
	}
	public void move() {
		this.x += this.velX;
	}
	public static void main(String[] args) 
	{
		Arrow a1 = new Arrow(150,"¡ß" ,10);
		a1.move();
	}
}
